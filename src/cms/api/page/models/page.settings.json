{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "page"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "cards": {
      "type": "dynamiczone",
      "components": [
        "card.welcome",
        "card.about-me",
        "card.contact-me",
        "card.portfolio",
        "card.wrap-image",
        "card.markdown"
      ]
    },
    "url": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "footer": {
      "model": "footer"
    },
    "header": {
      "model": "header"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published",
        "archived"
      ],
      "required": true,
      "private": true
    },
    "published_at": {
      "type": "datetime",
      "private": true
    },
    "deployed_at": {
      "type": "datetime"
    }
  }
}
