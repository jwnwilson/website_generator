{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "page"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "cards": {
      "type": "dynamiczone",
      "components": [
        "card.welcome",
        "card.about-me",
        "card.contact-me",
        "card.portfolio",
        "card.wrap-image"
      ]
    },
    "header": {
      "type": "component",
      "repeatable": false,
      "component": "header.header-1"
    },
    "url": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "footer": {
      "type": "component",
      "repeatable": false,
      "component": "footer.footer"
    }
  }
}
